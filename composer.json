{
    "name": "birdcode/bc-simplerate",
    "type": "typo3-cms-extension",
    "description": "Include rate feature for any record",
    "keywords": [
        "TYPO3",
        "extension",
        "rate"
    ],
    "homepage": "https://birdcode.tech/",
    "authors": [
        {
            "name": "Bird Dev",
            "role": "Developer",
            "homepage": "https://birdcode.tech"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "php": ">= 8.0 < 8.4", 
        "typo3/cms-backend": "^13.4.5",
        "typo3/cms-core": "^13.4.5",
        "typo3/cms-extbase": "^13.4.5",
        "typo3/cms-frontend": "^13.4.5",
        "typo3/cms-fluid": "^13.4.5"
    },
    "autoload": {
        "psr-4": {
            "BirdCode\\BcSimplerate\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/bc-simplerate": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "lock": false,
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "bc_simplerate",
            "app-dir": ".Build",
            "web-dir": ".Build/Web"
        }
    }
}